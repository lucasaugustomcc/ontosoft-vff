@prefix : <http://ontosoft.org/software#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://ontosoft.org/software> .

<http://ontosoft.org/software> rdf:type owl:Ontology ;
                                owl:imports <http://ontosoft.org/softwareCategories> ;
                                <http://purl.org/dc/terms/created> "2015-03-12"^^xsd:date ;
                                <http://purl.org/dc/terms/description> "Ontosoft is an ontology to describe software."@en ;
                                <http://purl.org/dc/terms/modified> "2018-04-21"^^xsd:string ;
                                <http://purl.org/dc/terms/title> "The Ontosoft ontology"@en ;
                                <http://purl.org/vocab/vann/preferredNamespacePrefix> "osw"^^xsd:string ;
                                <http://purl.org/vocab/vann/preferredNamespaceUri> "http://ontosoft.org/software#"^^xsd:string ;
                                rdfs:comment "Ontosoft is an ontology to describe software"@en ;
                                owl:versionInfo "2.0"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://ontosoft.org/software#category
:category rdf:type owl:AnnotationProperty ;
          rdfs:comment "Category of software metadata properties"@en .


###  http://ontosoft.org/software#isRequired
:isRequired rdf:type owl:AnnotationProperty ;
            rdfs:comment "This property annotation indicates if a property is a required property or not"@en .


###  http://ontosoft.org/software#searchConfig
:searchConfig rdf:type owl:AnnotationProperty ;
              rdfs:comment "This property annotation provides information to be handed over to search engines (like Solr) to explain how to tokenize and index the property values"@en .


###  http://ontosoft.org/software#uiConfig
:uiConfig rdf:type owl:AnnotationProperty ;
          rdfs:comment "This property annotation provides UI specific information for the property like positioning, special UI interface, etc"@en .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#description
rdfs:description rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://ontosoft.org/software#affectsSoftwareFunction
:affectsSoftwareFunction rdf:type owl:ObjectProperty ;
                         rdfs:domain :KnownIssue ;
                         rdfs:range :Function ;
                         :category <http://ontosoft.org/softwareCategories#Trust> ;
                         :uiConfig "{\"position\": 3}"^^xsd:string .


###  http://ontosoft.org/software#changesSoftwareFunction
:changesSoftwareFunction rdf:type owl:ObjectProperty ;
                         rdfs:domain :SoftwareChange ;
                         rdfs:range :Function ;
                         :category <http://ontosoft.org/softwareCategories#Track> ;
                         :uiConfig "{\"position\": 2}"^^xsd:string .


###  http://ontosoft.org/software#fixedKnownIssue
:fixedKnownIssue rdf:type owl:ObjectProperty ;
                 rdfs:domain :BugFix ;
                 rdfs:range :KnownIssue .


###  http://ontosoft.org/software#hasActiveDevelopment
:hasActiveDevelopment rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :Software ;
                      rdfs:range :DevelopmentInformation ;
                      :category <http://ontosoft.org/softwareCategories#Contribute> ;
                      :uiConfig "{\"position\":1}"^^xsd:string ;
